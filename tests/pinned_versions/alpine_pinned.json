{"BASE_IMAGE" : "alpine",
 "BASE_TAG" : "3.18.2",
 "NEED_SPECIAL_TAGS" : {"install_from_testing" : "edge"},

 "BUILDER_DEFAULT_BINARIES_DIR": "/tmp/runner_root_dir", 
 "RUNNER_BINARIES_DIR" : "/",

 "BUILD_STAGES" : {
    "rc" :       {"BUILD_SCRIPT" : "",
                  "BUILD_DOCKERFILE_TEMPLATE" : "./build_stage_templates/alpine/rc/Dockerfile_plan9_build_stage.jinja"
                 }
 },

 "COMMANDS" : {
      "install_from_main_and_community" : {
       "command_template" : "/command_templates/apk.jinja",
       "supported_parameters" : {"bash" : "=9.6.7",
                                 "dash" : "=3.4.5",
                                 "fish": "=2.3.4"
                                },
       "prefix" : "\\\n    ",
       "separator" : " \\\n    " },
      "install_from_testing" : {
       "command_template" : "/command_templates/alpine-apk-testing.jinja",
       "supported_parameters" : {"elvish" : "=1.8.9",
                                 "xonsh" : "=4.8.7"
                                },
       "prefix" : " \\\n    ",
       "separator" : "\\\n    " }
                              },

 "COMMANDS_ORDER" : ["install_from_main_and_community",
                     "install_from_testing",
                     "BUILD_STAGES"
                    ],
 "UNUSED_PARAMETERS_COMMAND" : "install_from_main_and_community"
}