{"BASE_IMAGE" : "ubuntu",
 "BASE_TAG" : "jammy",
 "NEED_SPECIAL_TAGS" : {},

 "BUILDER_DEFAULT_BINARIES_DIR": "/tmp/runner_root_dir", 
 "RUNNER_BINARIES_DIR" : "/",

 "BUILD_STAGES" : {
    "heirloom" : {"BUILD_SCRIPT" : "",
                  "BUILD_DOCKERFILE_TEMPLATE" : "source_builds/debian/heirloom/Dockerfile_heirloom_build_stage.jinja"
                 }
 },

 "COMMANDS" : {
      "install_from_default_repos" : {
          "command_template" : "/commands/apt-get.jinja",
          "supported_parameters" : [
                                 "gcc",
                                 "make",
                                 "cmake",
                                 "g++",
                                 "libboost-dev",
                                 "ca-certificates",
                                 "git"
                                ],
          "prefix" : "\\\n    ",
          "separator" : " \\\n    " }
     },

 "COMMANDS_ORDER" : ["install_from_default_repos",
                     "BUILD_STAGES"
                    ],
 "UNUSED_PARAMETERS_COMMAND" : "install_from_default_repos"

}