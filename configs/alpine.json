{"BASE_IMAGE" : "alpine",
 "BASE_TAG" : "3.18.2",
 "NEED_SPECIAL_TAGS" : {"install_from_testing" : "edge"},

 "BUILDER_DEFAULT_BINARIES_DIR": "/tmp/runner_root_dir", 
 "RUNNER_BINARIES_DIR" : "/",

 "BUILD_STAGES" : {
    "rc" :       {"BUILD_SCRIPT" : "",
                  "BUILD_DOCKERFILE_TEMPLATE" : "./source_builds/alpine/rc/Dockerfile_plan9_build_stage.jinja"
                 }
 },

 "COMMANDS" : {
      "install_from_main_and_community" : {
       "format_string" : "RUN apk add --no-cache {parameters}", 
       "command_template" : "/commands/apk.jinja",
       "supported_parameters" : ["bash",
                                 "dash",
                                 "fish",
                                 "zsh",
                                 "ion-shell",
                                 "tcsh",
                                 "oksh",
                                 "loksh",
                                 "yash",
                                 "rssh"
                                ],
       "prefix" : "\\\n    ",
       "separator" : " \\\n    " },
      "install_from_testing" : {
       "format_string" : "RUN apk add --no-cache --repository=https://dl-cdn.alpinelinux.org/alpine/edge/testing/ {parameters}", 
       "command_template" : "/commands/alpine-apk-testing.jinja",
       "supported_parameters" : ["elvish",
                                 "hilbish",
                                 "nsh",
                                 "xonsh",
                                 "mrsh",
                                 "imrsh",
                                 "nushell"
                              ],
       "prefix" : " \\\n    ",
       "separator" : "\\\n    " }
                              },

 "COMMANDS_ORDER" : ["install_from_main_and_community",
                     "install_from_testing",
                     "BUILD_STAGES"
                    ],
 "UNUSED_PARAMETERS_COMMAND" : "install_from_main_and_community"
}